  "version": "0.2.0",
  "configurations": [

    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args":[
        "info",
        // "--debugEnabled",
        // "true",
        "--namespace",
        "fromFlag",
        // "--project",
        // "fromFlag",
      ],
      "env": {
        "CLI_NAMESPACE": "fromEnv",
        "CLI_PROJECT": "fromEnv",
        "CLI_DEBUGENABLED": "true",
      }
    }
  ]
}